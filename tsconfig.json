{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "alwaysStrict": true,
    "declaration": true,
    "declarationMap": true,
    "forceConsistentCasingInFileNames": true,
    "keyofStringsOnly": true,
    "useDefineForClassFields": true,
    "target": "ES2020",
    "strict": true,
    "outDir": "build"
  }
}
